

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>@ViewData["Title"]</title>
    <link href="~/ace/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/ace/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/ace/css/ace.min.css" rel="stylesheet" />
    <link href="~/ace/css/ace-fonts.css" rel="stylesheet" />
    <link href="~/ace/css/ace.min.css" rel="stylesheet" />
    <link href="~/ace/css/ace-skins.min.css" rel="stylesheet" />
    <link href="~/ace/css/ace-rtl.min.css" rel="stylesheet" />
    <link href="~/ace/css/ace-part2.min.css" rel="stylesheet" />
    <link href="~/ace/css/bootstrap-editable.css" rel="stylesheet" />
    <link href="~/ace/css/colorbox.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" asp-append-version="true" />
    <script src="~/ace/js/ace-extra.min.js"></script>
    <script src="~/ace/js/jquery.min.js"></script>
</head>
<body class="no-skin">
    <div id="navbar" class="navbar navbar-default navbar-fixed-top">
        <div class="navbar-container" id="navbar-container">
            <button type="button" class="navbar-toggle menu-toggler pull-left" id="menu-toggler">
                <span class="sr-only">Toggle sidebar</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <div class="navbar-header pull-left">
                <a href="@Url.RouteUrl("mainIndex")" class="navbar-brand">
                    <small>
                        <i class=""></i>
                        @SiteContext.Settings.SiteName
                    </small>
                </a>
            </div>
            <partial name="_LoginPartial" />
        </div>
    </div>
    <div class="main-container" id="main-container">
        <partial name="_NavbarPartial" />
        <div class="main-content">
            <partial name="_Breadcrumbs" />
            @RenderBody()
        </div>
    </div>
    <div class="footer">
        <div class="footer-inner">
            <!-- #section:basics/footer -->
            <div class="footer-content">
                <span class="bigger-120">
                    <span class="blue bolder">Ace</span>
                    @SiteContext.Settings.SiteName © 2019-@DateTime.Now.Year
                </span>
            </div>
            <!-- /section:basics/footer -->
        </div>
    </div>
    <script src="~/ace/js/bootstrap.min.js"></script>
    <script src="~/ace/js/ace-elements.min.js"></script>
    <script src="~/ace/js/ace.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
    <script src="~/ace/js/x-editable/bootstrap-editable.min.js"></script>
    <script src="~/ace/js/fuelux/fuelux.spinner.min.js"></script>
    <script src="~/ace/js/x-editable/ace-editable.min.js"></script>
    <script src="~/layer/layer.js"></script>
    <script src="~/js/jquery.form.js"></script>
    <script src="~/ace/js/jquery.dataTables.min.js"></script>
    <script src="~/ace/js/jquery.dataTables.bootstrap.js"></script>
    <script src="~/ace/js/jquery.colorbox-min.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
