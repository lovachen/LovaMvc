@model Sys_NLogMapping
<div class="space-10"></div>
<div class="col-sm-12">

    <dl class="dl-horizontal"> 
        <dt>Level</dt>
        <dd>@Model.level</dd>
        <dt>时间</dt>
        <dd>@Model.logged_format</dd>
        <dt>Message</dt>
        <dd>@Model.message</dd>
        <dt>来源类</dt>
        <dd>@Model.logger</dd>
        <dt>Callsite</dt>
        <dd>@Model.callsite</dd>
        <dt>Exception</dt>
        <dd>@Model.exception</dd>
    </dl>

</div>






